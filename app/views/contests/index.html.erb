<h1><%=image_tag('logo')%></h1>

<% if session[:user_id] %>
<%= User.find(session[:user_id]).username %>
<%= button_to "Log out", logout_path %>
<% else %>
<%= link_to "Log in", login_path %>
<%= link_to "Sign up", new_user_path %>
<% end %>



<div class="container">
        <ul class="list">
        <div class="animation">
                <h1> CONTESTS </h1><br> 
                        </div>
                <% Contest.biggest_prize.each do |contest| %>
                <li><%= link_to contest.name, contest_path(contest) %> <%= "$ " + contest.prize.to_s %> <%= link_to "  Enter to win", new_team_path %></li><br>
                <% end %>
         </ul>
</div>